#include<bits/stdc++.h>
using namespace std;

string reverse(string str,string str_rev)
{
     for(int i=str.size()-1;i>=0;i--)
    {
        str_rev.push_back(str[i]);
    }return str_rev;
}

int main()
{
    int n;
    cin>>n;
    string str;
    string str_rev;
    while(n--)
    {
        cin>>str;
        cout<<reverse(str,str_rev)<<endl;
    }
}
